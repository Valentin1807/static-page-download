$(document).ready(function(){
    $('.sliderImages').slick({
        fade: true,
        arrows: false,
        dots: true,
        autoplay: true,
        autoplaySpeed:5000
    });
    $('button#pause').on('click', function() {
        $('.sliderImages').slick('slickPause');
        $('.sliderText').slick('slickPause');
        $(this).hide();
        $('button#play').show();
    });
    $('button#play').on('click', function() {
        $('.sliderImages').slick('slickPlay');
        $('.sliderText').slick('slickPlay');
        $(this).hide();
        $('button#pause').show();
    })
});
</script>




