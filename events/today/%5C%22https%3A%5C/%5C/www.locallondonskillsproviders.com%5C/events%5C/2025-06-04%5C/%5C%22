<script>

$( ".ugb-video-popup__wrapper" ).click(function() {
	if (typeof gtag === "function")
	{
		gtag('event', 'video_'+$(this).data("video"));
	}
	else if (typeof ga === "function")
	{
		ga('send', {
  			hitType: 'event',
  			eventCategory: 'Video_'+$(this).data("video"),
  			eventAction: 'play',
  			eventLabel: $(this).data("video")
		});
	}
});

